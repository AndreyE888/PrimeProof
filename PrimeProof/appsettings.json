{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ApplicationSettings": {
    "Name": "PrimeProof",
    "Version": "1.0.0",
    "Description": "Система тестирования простоты чисел для криптографии",
    "MaxNumberLength": 1000,
    "DefaultRounds": 10,
    "RecommendedRounds": {
      "TrialDivision": 1,
      "Fermat": 20,
      "MillerRabin": 40,
      "AKS": 1
    },
    "Features": {
      "EnableQuickCheck": true,
      "EnableResultExport": true,
      "EnableTestComparison": true
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db"
  }
}